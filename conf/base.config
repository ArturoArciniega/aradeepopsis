/* -------------------------------------------------
 * Nextflow config file for aradeepopsis
 * -------------------------------------------------
 */

env {
  PYTHONPATH = "${PYTHONPATH}:$baseDir/bin"
}

process {
  withName: build_TFRecords {
  	cpus = { 1 * task.attempt }
  	memory = { 1.GB * task.attempt }
  	time = { 1.h * task.attempt }
  }
}
